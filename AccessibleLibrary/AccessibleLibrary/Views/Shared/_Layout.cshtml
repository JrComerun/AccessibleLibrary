
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Accessible Library</title>
    <link rel="icon" href="~/src/img/logo/nav-logo.png" type="image/png" sizes="16x16" />
    <script src="~/src/js/Font_Awesome/icons.js"></script>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;1,500;1,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/src/css/bootsrap/bootstrap-grid.css" />
    <link rel="stylesheet" href="~/src/css/bootsrap/bootstrap-reboot.css" />
    <link rel="stylesheet" href="~/src/css/bootsrap/bootstrap.css" />
    <link rel="stylesheet" href="~/src/css/style.css" />
    <link rel="stylesheet" href="~/src/css/backend.css" />
    @RenderSection("Css", required: false)
    <title>@ViewData["Title"] - AccesiblelLibrary</title>
</head>
<body>
    <!-- *-*-*-*-*-*-*-*-*      Header Start      *-*-*-*-*-*-*-*-* -->
    @await Component.InvokeAsync("Header")
    <!-- *-*-*-*-*-*-*-*-*      Header End      *-*-*-*-*-*-*-*-* -->
    <!-- ---------------------------------------------------------- -->
    <main>
        @RenderBody()
    </main>

    <!-- *-*-*-*-*-*-*-*-*      Footer Start      *-*-*-*-*-*-*-*-* -->
    @await Component.InvokeAsync("Footer")
    <!-- *-*-*-*-*-*-*-*-*      Footer End      *-*-*-*-*-*-*-*-* -->
    <!-- ---------------------------------------------------------- -->
    <!-- *-*-*-*-*-*-*-*-*      Components Start      *-*-*-*-*-*-*-*-* -->
    <partial name="_PartialComponents" />

    <!-- ---------------------------------------------------------- -->
    <!-- *-*-*-*-*-*-*-*-*      Selected Start      *-*-*-*-*-*-*-*-* -->
    @RenderSection("SelectHolder1", required: false)
    <!-- ---------------------------------------------------------- -->
    @RenderSection("SelectHolder2", required: false)

    <!-- *-*-*-*-*-*-*-*-*      Selected End      *-*-*-*-*-*-*-*-* -->
    <!-- ---------------------------------------------------------- -->
    <!-- *-*-*-*-*-*-*-*-*      Components End      *-*-*-*-*-*-*-*-* -->

    @if (User.Identity.IsAuthenticated)
    {
        <script>
            var userAuthorized = true;
        </script>

    }
    else
    {
        <script>
            var userAuthorized = false;
        </script>

    }
    <script src="~/src/js/jquery/jquery.js"></script>
    <script src="~/src/js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/src/js/bootstrap/bootstrap.min.js"></script>
    <script src="~/src/js/mobile.js"></script>
    <script src="~/src/js/main.js"></script>
    <script src="~/src/js/backend.js"></script>
    @RenderSection("Scripts", required: false)
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>
